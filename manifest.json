{
  "manifest_version": 3,
  "name": "Extract Tables from Image",
  "version": "1.0",
  "description": "Extract tables from images on the web",
  "permissions": [
    "contextMenus",
    "activeTab",
    "scripting",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon32.png",
      "128": "icon192.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon32.png",
    "128": "icon192.png"
  }
}
